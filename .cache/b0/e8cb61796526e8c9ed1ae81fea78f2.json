products\">\n          {products.map((product) => (\n            <p className=\"items\" key={product.id}>\n              Marque :{product.brands}\n              <img\n                className=\"images\"\n                key={product.id}\n                src={product.image_front_small_url}\n                alt={\"\"}\n              />\n            </p>\n          ))}\n        </div>\n      )}\n    </div>\n  );\n};\n\nexport default SearchFunc;"},"lineCount":null}},"error":null,"hash":"1ae4f025a8db6fdfb2769084dc34e1cc","cacheData":{"env":{}}}